{
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
  "terminal.integrated.shell.linux": "/bin/bash",

  "window.zoomLevel": 0,

  "breadcrumbs.enabled": true,

  "workbench.colorTheme": "Dracula",
  "workbench.editor.labelFormat": "short",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",

  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "editor.lineHeight": 26,
  "editor.fontFamily": "Fira Code Retina",
  "editor.fontLigatures": true,
  "editor.renderLineHighlight": "gutter",
  "editor.parameterHints.enabled": false,
  "editor.formatOnSave": true,
  "editor.suggestSelection": "first",
  "editor.rulers": [80, 120],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },

  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.idea": true,
    "**/.vstudio": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "node_modules": true
  },

  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,

  "extensions.ignoreRecommendations": true,

  "prettier.enable": true,

  "emmet.syntaxProfiles": { "javascript": "jsx" },
  "emmet.includeLanguages": { "javascript": "javascriptreact" },

  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.autoImports": true,
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "typescript.updateImportsOnFileMove.enabled": "never",
  "typescript.tsserver.log": "verbose",
  "typescript.suggest.autoImports": true,
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "files.associations": {
    ".sequelizerc": "javascript",
    ".stylelintrc": "json",
    ".prettierrc": "json"
  },

  "git.enableSmartCommit": true,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,

  "liveshare.featureSet": "insiders",

  "liveServer.settings.donotShowInfoMsg": true,
  
  "screencastMode.onlyKeyboardShortcuts": true,

  "material-icon-theme.folders.associations": {
    "infra": "app",
    "entities": "class",
    "schemas": "class",
    "typeorm": "database",
    "repositories": "mappings",
    "http": "container",
    "migrations": "tools",
    "modules": "components",
    "implementations": "core",
    "dtos": "typescript",
    "fakes": "mock",
    "websockets": "pipe",
    "protos": "pipe",
    "grpc": "pipe"
  },

  "material-icon-theme.files.associations": {
    "ormconfig.json": "database",
    "tsconfig.json": "tune",
    "*.proto": "3d"
  },

  "mssql.connections": [
    {
      "server": "(localdb)\\MSSQLLocalDB",
      "database": "",
      "authenticationType": "Integrated",
      "profileName": "MSSQLLocalDB",
      "password": "",
      "azureAccountToken": ""
    }
  ],

  "vs-kubernetes": {
    "vs-kubernetes.helm-path.linux": "/home/dnl/.vs-kubernetes/tools/helm/linux-amd64/helm",
    "vs-kubernetes.minikube-path.linux": "/home/dnl/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
    "vs-kubernetes.draft-path.linux": "/home/dnl/.vs-kubernetes/tools/draft/linux-amd64/draft"
  },

  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
}